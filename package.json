    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "sonner": "^1.5.0",
    "toastify-react-native": "^5.0.2"
