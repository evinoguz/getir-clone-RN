    "expo-linking": "^6.3.1",
    "expo-status-bar": "~1.12.1",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.4",
    "react": "18.2.0",
    "react-dom": "^18.3.1",
    "react-native": "0.74.5",
